<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>SFA.DAS.WebUIStyling</id>
        <version>1.0.1</version>
        <authors>DAS</authors>
        <owners>DAS</owners>
        <licenseUrl>https://github.com/SkillsFundingAgency/das-shared-packages/blob/master/LICENSE</licenseUrl>
		<projectUrl>https://github.com/SkillsFundingAgency/das-shared-packages</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Styling assests for DAS web projects</description>
    </metadata>
    <files>
        <file src="assets\images\*.*" target="content\Content\images" />
        <file src="assets\javascripts\*.*" target="content\Scripts" />
        <file src="assets\stylesheets\*.*" target="content\Content\stylesheets" />
        <file src="assets\stylesheets\images\*.*" target="content\Content\stylesheets\images" />
        <file src="assets\stylesheets\fonts\*.*" target="content\Content\stylesheets\fonts" />
        <file src="assets\stylesheets\external-links\*.*" target="content\Content\stylesheets\external-links" />
    </files>
</package>