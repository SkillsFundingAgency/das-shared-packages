@* Generator: MvcView<object> *@
@model SFA.DAS.MA.Shared.UI.Models.IFooterViewModel
@using SFA.DAS.MA.Shared.UI.Models.Links

<footer class="group js-footer" id="footer" role="contentinfo">
    <div class="footer-wrapper">
        <div class="footer-meta">
            <div class="footer-meta-inner">
                <ul class="platform-help-links">
                    @Html.Raw(Model.RenderListItemLink<Help>())
                    @Html.Raw(Model.RenderListItemLink<Feedback>())
                    @Html.Raw(Model.RenderListItemLink<Privacy>())
                    @Html.Raw(Model.RenderListItemLink<Cookies>())
                    <li>Built by the @Html.Raw(Model.RenderLink<BuiltBy>())</li>
                </ul>
                <div class="open-government-licence">
                    <p class="logo">
                        @Html.Raw(Model.RenderLink<OpenGovernmentLicense>())
                    </p>
                    <p>
                        All content is available under the @Html.Raw(Model.RenderLink<OpenGovernmentLicenseV3>()), except where otherwise stated
                    </p>
                </div>
            </div>
            <div class="copyright">
                @Html.Raw(Model.RenderLink<CrownCopyright>())
            </div>
        </div>
    </div>
</footer>

