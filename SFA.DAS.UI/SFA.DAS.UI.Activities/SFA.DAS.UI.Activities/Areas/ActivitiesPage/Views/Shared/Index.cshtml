@model SFA.DAS.UI.Activities.Areas.ActivitiesPage.Models.ActivitiesListModel
    
<table>
    <thead>
    <tr>
        <th>Activity Type</th><th>Description</th>
    </tr>
    </thead>
    @foreach (var activity in Model.Activities)
    {
        <tr>
            <td>
                @activity.DayText
            </td>
            <td>
            </td>
            <td>
                <b>@activity.LineOne</b>
            </td>
            <td>
                @activity.LineTwo
            </td>
        </tr>
    }
</table>